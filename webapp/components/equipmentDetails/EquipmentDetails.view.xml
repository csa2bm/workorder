<core:View controllerName="com.twobm.mobileworkorder.components.equipmentDetails.EquipmentDetails" 
xmlns:core="sap.ui.core"
xmlns:html="http://www.w3.org/1999/xhtml" 
xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" 
xmlns:m="sap.m" 
xmlns="sap.uxap"
xmlns:blocks="com.twobm.mobileworkorder.components.equipmentDetails.blocks"
height="100%">
	
	<m:Page showNavButton="true" navButtonPress="onNavigationButtonPress" title="{i18n>EquipmentDetailsTitle}" height="100%">
		<ObjectPageLayout id="equipmentObjectPage" enableLazyLoading="false">
			<headerTitle>
				<ObjectPageHeader headerDesign="Dark" objectTitle="{path:'Equidescr'}" objectSubtitle="{path:'Equipment'}"/>
			</headerTitle>
			<sections>
				
				<ObjectPageSection title="{i18n>SubSectionTitleOperationInformation}">
					<subSections>
						<ObjectPageSubSection title="{i18n>SubSectionTitleOperationInformation}">
							<blocks>
								<blocks:EquipmentInformationBlock/>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				<!--
				<ObjectPageSection title="{i18n>SubSectionTitleOperationNotes}">
					<subSections>
						<ObjectPageSubSection title="{i18n>SubSectionTitleOperationNotes}">
							<blocks>
								<blocks:OperationNotesBlock/>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				-->
					<ObjectPageSection title="{i18n>SubSectionTitleMesuringPoints}">
					<subSections>
						<ObjectPageSubSection title="{i18n>SubSectionTitleMesuringPoints}">
							<blocks>
								<blocks:MeasuringPointsBlock/>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				<ObjectPageSection title="{i18n>SubSectionTitleOrderHistory}">
					<subSections>
						<ObjectPageSubSection title="{i18n>SubSectionTitleOrderHistory}">
							<blocks>
								<blocks:OrderHistoryBlock/>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
					<ObjectPageSection title="{i18n>SubSectionTitleNotificationHistory}">
					<subSections>
						<ObjectPageSubSection title="{i18n>SubSectionTitleNotificationHistory}">
							<blocks>
								<blocks:NotificationHistoryBlock/>
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				</sections>
			</ObjectPageLayout>
				<m:footer>
			<m:OverflowToolbar>
				<m:content>
					<m:ToolbarSpacer></m:ToolbarSpacer>
					<m:Button id="createNotification" 
						press="onPressCreateNotification" text="Create Notification"
						 type="Emphasized">
						<m:layoutData>
							<m:OverflowToolbarLayoutData priority="NeverOverflow"/>
						</m:layoutData>
					</m:Button>
				</m:content>
			</m:OverflowToolbar>
		</m:footer>
		</m:Page>
	</core:View>