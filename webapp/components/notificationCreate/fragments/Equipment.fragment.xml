<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id = "EquipmentsLocationsDialog"
		title="{i18n>NotificationCreate-Equipments}"
		class="sapUiPopupWithPadding"
		items="{/EquipmentsSet}"
		search="_handleValueHelpSearchEquipment"
		confirm="_handleValueHelpCloseEquipment"
		cancel="_handleValueHelpClose"
		>
		<StandardListItem
			title="{Equipment}"
			description="{Description}" />
	</SelectDialog>
</core:FragmentDefinition>