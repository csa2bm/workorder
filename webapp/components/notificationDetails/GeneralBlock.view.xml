<core:View controllerName="com.twobm.mobileworkorder.components.notificationDetails.GeneralBlock" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:vbm="sap.ui.vbm" xmlns="sap.m">
<VBox class="sapUiSmallMargin">
<f:SimpleForm maxContainerCols="3"
			editable="false"
			layout="ResponsiveGridLayout"
			title=""
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="4"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			columnsXL="3"
			columnsL="2"
			columnsM="2"
			class="sapUiTinyMarginBottom"
			>
		<f:content>
			<!--Column Description-->
			<core:Title text="{i18n>GeneralBlockColoumInformation}"/>
			<!--<Label text="{i18n>GeneralBlockColoumInformationLabelNumber}"/>
			<Text text="{Orderid} "/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelShortText}"/>
			<Text text="{ShortText} "/>
<!-->
			<Label text="{i18n>GeneralBlockColoumInformationLabelType}"/>

			<Text text="{NotifType}"/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelPriority}"/>
			<Text text="{Priority} "/>
	<!--		<Label text="{i18n>GeneralBlockColumnInformationLabelResponsible}"/>
			<Text text="{ChangedBy}\r\n({Respcctr})"/>-->
			<Label text="{i18n>GeneralBlockColumnInformationLabelOrderStatus}"/>
			<Text text="{SysStatus}"/>
				<Label text="{i18n>GeneralBlockColumnDateAndTimeLabelStart}"/>
			<Text text="{path: 'RequiredStartDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"/>
			<Label text="{i18n>GeneralBlockColumnDateAndTimeLabelEnd}"/>
			<Text text="{path: 'RequiredEndDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"/>
			<!--Column Time and Place-->
			<core:Title text="{i18n>GeneralBlockColumnTimePlace}"/>
		

		<Label text="{i18n>GeneralBlockColumnEquipmentLabelFunctionalLocation}"/>
			<Text text="{FuncLocDesc}\r\n({FunctionalLoc})"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelEquipment}"/>
			<Text text="{EquipmentDesc}\r\n({Equipment})"/>
		<!--	<Label text="{i18n>GeneralBlockColumnEquipmentLabelAddress}"/>
			<Text text="{Street}\r\n{City}"/>-->
		
			<!--<Label text="{i18n>GeneralBlockColumnEquipmentLabelCity}"/>-->
			<!--<Text text=""/>-->
			<!--Column Equipment-->
	<!--		<core:Title text="{i18n>GeneralBlockColumnEquipment}"/>-->

		</f:content>
	</f:SimpleForm>
	</VBox>
</core:View>