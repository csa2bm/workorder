<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id = "CodeValueHelpDialog"
		title="{i18n>NotificationDetails-ActivityCodeValueHelp-Title}"
		class="sapUiPopupWithPadding"
		items="{CodeCatalogSet}"
		search="handleActivityCodeSearch"
		confirm="handleValueHelpCloseCode"
		cancel="handleValueHelpCloseCode"
	>
		<StandardListItem
			title="{CodeText}"
			description="{Code}" />
	</SelectDialog>
</core:FragmentDefinition>