<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Attachment" class="sapUiPopupWithPadding" stretch="false" showHeader="false">
		<beginButton>
			<Button icon="sap-icon://delete" type="Reject" text="{i18n>NotificationDetails-AttachmentPopover-DeleteButton}" press="onDeleteFile" visible="{parts:[{path:'ImageModel>/AttachmentID'}],formatter:'.attachmentPopUpShowDeleteButton'}"></Button>
		</beginButton>
		<endButton>
			<Button text="{i18n>NotificationDetails-AttachmentPopover-CloseButton}" press="closeAttachmentPopupButton"/>
		</endButton>
		<buttons></buttons>
		<content>
			<VBox>
				<Image src="{ImageModel>/ImagePath}" height="{ImageModel>/ImageHeight}"/>
		
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>