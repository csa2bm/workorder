<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id = "DamageCodeValueHelpDialog"
		title="{i18n>NotificationDetails-ItemDamageCodeValueHelp}"
		class="sapUiPopupWithPadding"
		items="{CodeCatalogSet}"
		
		confirm="handleValueDamageHelpCloseCode"
		cancel="handleValueHelpAfterCloseCode"
		>
		<!--afterClose="handleValueHelpAfterCloseCode"-->
		<StandardListItem
			title="{CodeText}"
			description="{Code}" />
	</SelectDialog>
</core:FragmentDefinition>
