<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id = "CodeValueHelpDialog"
		title="{i18n>NotificationDetails-ItemPartCodeValueHelp}"
		class="sapUiPopupWithPadding"
		items="{CodeCatalogSet}"
		
		confirm="handleValueHelpCloseCode"
		cancel="handleValueHelpCloseCode"
		>
		<!--afterClose="handleValueHelpAfterCloseCode"-->
		<StandardListItem
			title="{CodeText}"
			description="{Code}" />
	</SelectDialog>
</core:FragmentDefinition>