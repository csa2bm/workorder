<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog class="sapUiPopupWithPadding" stretch="{device>/isPhone}" showHeader="true"
		title="{i18n>NotificationDetails-AttachmentPopover-Title}">
		<beginButton>
			<Button icon="sap-icon://delete" type="Reject" text="{i18n>NotificationDetails-AttachmentPopover-DeleteButton}" press="onDeletePhoto"
				visible="{parts:[{path:'ImageModel>/AttachmentID'}],formatter:'.photoPopUpShowDeleteButton'}"></Button>
		</beginButton>
		<endButton>
			<Button text="{i18n>NotificationDetails-AttachmentPopover-CloseButton}" press="closePhotoPopupButton"/>
		</endButton>
		<buttons></buttons>
		<content>
			<VBox>
				<Image src="{ImageModel>/ImagePath}" height="{ImageModel>/ImageHeight}"/>
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>