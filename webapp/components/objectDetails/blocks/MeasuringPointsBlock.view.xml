<core:View controllerName="com.twobm.mobileworkorder.components.objectDetails.blocks.MeasuringPointsBlock" xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:vbm="sap.ui.vbm" xmlns="sap.m">
	<Table id="idProductsTable" items="{ObjectMeasurementPoint}" noDataText="{i18n>ObjectDetails-MeasuringPointBlock-NoMeasuringPoints}">
		<columns>
			<Column>
				<Text text="{i18n>ObjectDetails-MeasuringPointBlock-MesuringPointColumn}"/>
			</Column>
			<Column>
				<Text text="{i18n>ObjectDetails-MeasuringPointBlock-PreviousColumn}"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem type="Active" press="onEquipmentItemPress">
				<cells>
					<ObjectIdentifier text="{Measpoint}" title="{Descriptn}"/>
					<ObjectIdentifier text="{LastReadingBy}" title="{LastReading} {MeasUnit}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:View>