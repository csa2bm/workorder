<core:View controllerName="com.twobm.mobileworkorder.components.objectDetails.blocks.OrderHistoryBlock" xmlns:core="sap.ui.core"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:vbm="sap.ui.vbm" xmlns="sap.m">
	<Table id="idOrderTable" items="{HistoricOrderSet}" noDataText="{i18n>OrderHistoryBlockNoHistory}" class="sapUiSmallMarginBottom">
		<columns>
			<Column>
				<Text text="{i18n>ObjectDetails-OrderHistoryBlock-TimeAndStatus}"/>
			</Column>
			<Column>
				<Text text="{i18n>ObjectDetails-OrderHistoryBlock-OrderType}"/>
			</Column>
			<Column>
				<Text text="{i18n>ObjectDetails-OrderHistoryBlock-Order}"/>
			</Column>
			<Column>
				<Text text="{i18n>ObjectDetails-OrderHistoryBlockC-User}"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem type="Active" press="onEquipmentItemPress">
				<cells>
					<ObjectIdentifier text="{UserStatus}" title="{}"/>
					<ObjectIdentifier text="{}" title="{OrderType}"/>
					<ObjectIdentifier text="{ShortText}" title="{OrderId}"/>
					<ObjectIdentifier text="{Workcenter}" title="{Plant}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:View>