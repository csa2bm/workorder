<core:View controllerName="com.twobm.mobileworkorder.components.workOrderDetails.EquipmentBlock" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
	<Table id="idProductsTable" items="{OrderObject}" noDataText="No equipment on this order">
		<columns>
			<Column>
				<Text text="Functional Location"/>
			</Column>
			<Column>
				<Text text="Equipment"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem type="Navigation">
				<cells>
					<ObjectIdentifier text="{Equidescr}" title="{Equipment}"/>
					<ObjectIdentifier text="{Funcldescr}" title="{FunclocDisp}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:View>