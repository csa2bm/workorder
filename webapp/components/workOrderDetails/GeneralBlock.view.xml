<core:View controllerName="com.twobm.mobileworkorder.components.workOrderDetails.GeneralBlock" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"
	xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:vbm="sap.ui.vbm" xmlns="sap.m">
<VBox class="sapUiSmallMargin">
<f:SimpleForm maxContainerCols="3"
			editable="false"
			layout="ResponsiveGridLayout"
			title=""
			labelSpanXL="4"
			labelSpanL="4"
			labelSpanM="4"
			emptySpanXL="0"
			emptySpanL="0"
			emptySpanM="0"
			columnsXL="3"
			columnsL="2"
			columnsM="2"
			>
		<f:content>
			<!--Column Description-->
			<core:Title text="{i18n>GeneralBlockColoumInformation}"/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelNumber}"/>
			<Text text="{Orderid} "/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelShortText}"/>
			<Text text="{ShortText} "/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelLongText}"/>
			<Text text="{OrderType}"/>
			<Label text="{i18n>GeneralBlockColoumInformationLabelPriority}"/>
			<Text text="{Priority} "/>
			<Label text="{i18n>GeneralBlockColumnInformationLabelResponsible}"/>
			<Text text="{Respcctr} - {ChangedBy}"/>
			<Label text="{i18n>GeneralBlockColumnInformationLabelOrderStatus}"/>
			<Text text="{SysStatus}"/>
			<!--Column Time and Place-->
			<core:Title text="{i18n>GeneralBlockColumnTimePlace}"/>
			<Label text="{i18n>GeneralBlockColumnDateAndTimeLabelStart}"/>
			<Text text="{path: 'StartDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"/>
			<Label text="{i18n>GeneralBlockColumnDateAndTimeLabelEnd}"/>
			<Text text="{path: 'FinishDate', type: 'sap.ui.model.type.DateTime', formatOptions: { style: 'medium' }}"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelStreet}"/>
			<Text text="{Street}"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelCity}"/>
			<Text text="{City}"/>
			<!--Column Equipment-->
			<core:Title text="{i18n>GeneralBlockColumnEquipment}"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelFLocNumber}"/>
			<Text text="{FunctLoc}"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelFLocShortText}"/>
			<Text text="{Funcldescr}"/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelEQNumber}"/>
			<Text text="{Equipment} "/>
			<Label text="{i18n>GeneralBlockColumnEquipmentLabelEQShortText}"/>
			<Text text="{Equidescr}"/>
		</f:content>
	</f:SimpleForm>
	</VBox>
</core:View>