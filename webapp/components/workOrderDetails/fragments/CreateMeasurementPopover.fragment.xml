<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Attachment" class="sapUiPopupWithPadding" stretch="false" showHeader="false" showCloseButton="true">
		<beginButton>
			<Button  type="Accept" text="{i18n>deleteAttachmentButtonText}" press="onDeleteFile" visible="{parts:[{path:'ImageModel>/AttachmentID'}],formatter:'.attachmentPopUpShowDeleteButton'}"></Button>
		</beginButton>
		<endButton>
			<Button text="Close" press="closeAddMeasurement"/>
		</endButton>
		<buttons></buttons>
		<content>
			<VBox>
				<!--class="sapUiSmallMargin"-->
				<!--<Label text="Select new work date" labelFor="DP3"/>-->
				<Label text="Enter reading" />
				<Input type="Text" />
			
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>