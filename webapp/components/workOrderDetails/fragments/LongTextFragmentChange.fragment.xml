<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="LongTextFragmentChangeId"
			minWidth="1024"
			maxContainerCols="2"
			editable="true"
			layout="ResponsiveGridLayout"
			title=""
			labelSpanL="3"
			labelSpanM="3"
			emptySpanL="4"
			emptySpanM="4"
			columnsL="1"
			columnsM="1"
			class="editableForm">
			<f:content>
				<Label text="LongText" />
				<Input value="{LongText}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>