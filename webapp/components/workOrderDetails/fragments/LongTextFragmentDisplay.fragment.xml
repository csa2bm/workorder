<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="LongTextFragmentDisplayId"
			minWidth="1024"
			maxContainerCols="2"
			editable="false"
			layout="ResponsiveGridLayout"
			title=""
			labelSpanL="4"
			labelSpanM="4"
			emptySpanL="1"
			emptySpanM="1"
			columnsL="1"
			columnsM="1">
			<f:content>
				<Label text="Longtext" />
				<Text text="{LongText}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>