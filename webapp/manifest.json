{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.twobm.mobileworkorder",
		"type": "application",
		"i18n": "i18n/messageBundle.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "ach",
		"dataSources": {
			"WORKORDER_SRV": {
				"uri": "/ID3/sap/opu/odata/tobmas/WORKORDER_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"offline": true
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://detail-view",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize_plus"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.twobm.mobileworkorder.components.dashboard.Dashboard",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.38.8",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.twobm.mobileworkorder.i18n.messageBundle"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "WORKORDER_SRV",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlId": "mobileWorkOrderApp",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "dashboard",
				"target": "dashboard"
			}, {
				"pattern": "workOrderList",
				"name": "workOrderList",
				"target": "workOrderList"
			}, {
				"pattern": "workOrderDetails/{workOrderContext}",
				"name": "workOrderDetails",
				"target": "workOrderDetails"
			}, {
				"pattern": "operationDetails/{operationContext}",
				"name": "operationDetails",
				"target": "operationDetails"
			}, {
				"pattern": "objectDetails/{objectContext}",
				"name": "objectDetails",
				"target": "objectDetails"
			}, {
				"pattern": "measurementPointDetails/{measurementContext}",
				"name": "measurementPointDetails",
				"target": "measurementPointDetails"
			}, {
				"pattern": "notificationList",
				"name": "notificationList",
				"greedy": false,
				"target": ["notificationList"]
			}, {
				"pattern": "notificationDetails/{notificationContext}",
				"name": "notificationDetails",
				"greedy": false,
				"target": ["notificationDetails"]
			}, {
				"pattern": "notificationCreate/:equipmentNo:/:functionalLoc:/:argAvailable:",
				"name": "notificationCreate",
				"greedy": false,
				"target": ["notificationCreate"]
			}, {
				"name": "objectTreeList",
				"pattern": "objectTreeList",
				"titleTarget": "",
				"greedy": false,
				"target": ["objectTreeList"]
			}],
			"targets": {
				"dashboard": {
					"viewPath": "com.twobm.mobileworkorder.components.dashboard",
					"viewName": "Dashboard",
					"viewLevel": 1
				},
				"workOrderList": {
					"viewPath": "com.twobm.mobileworkorder.components.workOrderList",
					"viewName": "WorkOrderList",
					"viewLevel": 2
				},
				"notFound": {
					"viewPath": "com.twobm.mobileworkorder.components.app",
					"viewName": "NotFound",
					"transition": "show"
				},
				"workOrderDetails": {
					"viewPath": "com.twobm.mobileworkorder.components.workOrderDetails",
					"viewName": "WorkOrderDetails",
					"viewLevel": 3
				},
				"operationDetails": {
					"viewPath": "com.twobm.mobileworkorder.components.operationDetails",
					"viewName": "OperationDetails",
					"viewLevel": 4
				},
				"objectDetails": {
					"viewPath": "com.twobm.mobileworkorder.components.objectDetails",
					"viewName": "ObjectDetails",
					"viewLevel": 4
				},
				"measurementPointDetails": {
					"viewPath": "com.twobm.mobileworkorder.components.measuringPointDetails",
					"viewName": "MeasuringPointDetails",
					"viewLevel": 4
				},
				"notificationList": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "NotificationList",
					"viewLevel": 2,
					"viewPath": "com.twobm.mobileworkorder.components.notificationList"
				},
				"notificationDetails": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "NotificationDetails",
					"viewLevel": 3,
					"viewPath": "com.twobm.mobileworkorder.components.notificationDetails"
				},
				"notificationCreate": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "NotificationCreate",
					"viewPath": "com.twobm.mobileworkorder.components.notificationCreate",
					"viewLevel": 5
				},
				"objectTreeList": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "objectTreeList",
					"viewLevel": 1,
					"viewPath": "com.twobm.mobileworkorder.components.objectTreeList"
				}
			}
		}
	},
	"sap.mobile": {
		"_version": "1.1.0",
		"definingRequests": {
			"OrderSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/OrderSet?$expand=OrderOperation,OrderComponent,OrderObject,OrderAttachments,OrderGoodsMovements,OrderTimeRegistration,OrderMaterialSummary,OrderEquipments,OrderMeasPoint"
			},
			"OperationsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/OperationsSet"
			},
			"AttachmentsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/AttachmentsSet"
			},
			"ComponentsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/ComponentsSet"
			},
			"GoodsMovementsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/GoodsMovementsSet"
			},
			"MaterialsSummarySet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/MaterialsSummarySet"
			},
			"ObjectsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/ObjectsSet"
			},
			"TimeRegistrationsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/TimeRegistrationsSet"
			},
			"MaterialsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": {
					"url": "/MaterialsSet",
					"retrieveStreams": true
				}
			},
			"EquipmentsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/EquipmentsSet"
			},
			"FunctionalLocationsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/FunctionalLocationsSet"
			},
			"NotificationsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/NotificationsSet?$expand=NotifAttachmentsSet,NotifActivitiesSet,NotifTasksSet"
			},
			"UserDetailsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/UserDetailsSet"
			},
			"CodeGroupsSet": {
				"dataSource": "WORKORDER_SRV",
				"path": "/CodeGroupsSet?$expand=CodeCatalogSet"
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}